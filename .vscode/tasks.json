{
	"version": "2.0.0",
	"tasks": [
		{
            "label": "Build Debug",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Temporal.sln"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Build Release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Temporal.sln",
                "-c",
                "Release"
            ],
            "problemMatcher": "$msCompile"
        },
		{
			"label": "Build Clean",
			"type": "dotnet",
			"task": "clean",
			"problemMatcher": [
				"$msCompile"
			]
		}
	]
}